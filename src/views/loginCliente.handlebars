<div class="container-fluid text-center">
    <div class="row content">
        <div class="col">
            <h1 class="text-align center">LOGIN CLIENTE</h1>
        </div>
    </div>
    <br>
    <div class="row content">
        <div class="col-sm-3 sidenav">
        </div>
        <div class="col-sm-6 text-justify">
            <form action="/login-cliente" method="post" name='formPJ' id="formPJ">
                <div class="form-group">
                    <label for="cpf">CPF</label>
                    <input class="form-control" name="cpf" id="cpf" type="text" onkeyup="cpfCheck(this)" maxlength="14"
                        onkeydown="javascript: fMasc( this, mCPF );"> <span id="cpfResponse"></span>
                </div>

                <div class="form-group">
                    <label for="senha">Senha</label>
                    <input class="form-control" type="password" name="senha" id="senha">
                </div>
                <div class="row">
                    <div class="col-2">
                        <button type="submit" class="btn btn-outline-primary">Entrar</button>
                    </div>
                    <div class="col">
                        <a class="a" href="/login-salao">DESEJO LOGAR SAL√ÉO</a>
                    </div>
                    <div class="col">
                        <a class="a" href="/cadastro-cliente">DESEJO CADASTRAR CLIENTE</a>
                    </div>
                </div>
            </form>
        </div>
        <div class="col-sm-3 sidenav">
        </div>
    </div>
</div>